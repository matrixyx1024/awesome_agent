{
  "model": {
    "provider": "openai",
    "model": "gpt-4",
    "apiKey": "sk-your-api-key-here",
    "temperature": 0.7,
    "maxTokens": 4096
  },
  "workspace": "./workspace",
  "maxConcurrent": 4,
  "timeoutSeconds": 600,
  "thinkingLevel": "medium",
  "verboseLevel": "off",
  "tools": [
    {
      "name": "browser",
      "enabled": true,
      "allowlist": [],
      "denylist": []
    },
    {
      "name": "filesystem",
      "enabled": true,
      "allowlist": [],
      "denylist": []
    },
    {
      "name": "exec",
      "enabled": true,
      "allowlist": ["ls", "cat", "grep", "find"],
      "denylist": ["rm -rf", "format", "del /f"]
    }
  ]
}
